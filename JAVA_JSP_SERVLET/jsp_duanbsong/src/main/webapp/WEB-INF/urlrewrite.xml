<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<rule>
		<from>/(.+)/page-([0-9]+).html$</from>
		<to>/public?index=$2</to>
	</rule>
	<rule>
		<from>/(.+)-([0-9]+).html$</from>
		<to>/detail?id=$2</to>
	</rule>
	<rule>
		<from>/(.+)-([0-9]+).html$</from>
		<to>/cats?id=$2</to>
	</rule>
	
</urlrewrite>

