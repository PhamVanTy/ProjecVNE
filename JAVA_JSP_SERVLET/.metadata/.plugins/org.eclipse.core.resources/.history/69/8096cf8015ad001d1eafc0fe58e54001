<script type="text/javascript">
	function load(cateid) {
		$.ajax({
			url: '<%=request.getContextPath()%>/public/cats',
			type: 'POST',
			cache: false,
			data: {
				cid: cateid
			},
			success: function(data){
				$('.main-content').html(data);
			},
			error: function (){
				alert('Có lỗi xảy ra');
			}
		});
	}
</script>
